a= float(input('ingrese el valor del primer lado del triangulo'))
b= float(input('ingrese el valor del segundo lado del triangulo'))
c= float(input('ingrese el valor del tercer lado del triangulo'))
if (a+b>c) and (a+c>b) and (b+c>a):
    if a==b and b==c:
        print('su triangulo es equilatero')
    elif (a==b and b!=c) or (a!=b and b==c) or (a==c and b!=c) :
        print('su triangulo es isoseles')
    elif (a!=b and b!=c and a!=c):
        print('su triangulo es escaleno')
else:
    print('su triangulo no es posible')
